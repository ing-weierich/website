{
  "name": "boilerplate",
  "version": "1.0.0-beta",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier:check": "node_modules/.bin/prettier --check './**/*.ts{,x}'",
    "prettier:format": "node_modules/.bin/prettier --write './**/*.ts{,x}'",
    "export:content-model": "contentful space export --skip-content --skip-roles --skip-webhooks --content-file=content-model.json --space-id",
    "check:all": "yarn prettier:check",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "contentful-cli": "^1.9.31",
    "cross-fetch": "^3.1.5",
    "framer-motion": "^6.2.6",
    "graphql": "^15.7.2",
    "husky": "^7.0.4",
    "next": "11.1.3",
    "next-pwa": "^5.4.0",
    "next-seo": "^4.28.1",
    "next-sitemap": "^1.6.203",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-breakpoints": "^10.0.1",
    "styled-components": "^5.3.3",
    "swiper": "^8.0.5"
  },
  "devDependencies": {
    "@types/react": "17.0.24",
    "@types/styled-components": "^5.1.15",
    "babel-plugin-styled-components": "^2.0.2",
    "dotenv": "^10.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "next-google-fonts": "^2.2.0",
    "react-scroll-parallax": "^3.0.2",
    "typescript": "4.4.3"
  }
}
